{
  "name": "nanobanana-cli",
  "version": "0.1.0",
  "description": "AI-powered image generation CLI using Google's Gemini API. Create, edit, and enhance images using natural language.",
  "type": "module",
  "bin": {
    "nb": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun --format esm",
    "start": "bun run dist/index.js",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run typecheck && bun run build"
  },
  "keywords": [
    "cli",
    "image-generation",
    "gemini",
    "ai",
    "image-editing",
    "nanobanana",
    "gemini-api",
    "ai-art",
    "image-cli",
    "bun"
  ],
  "author": "Dipendra Sharma",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dipendra-sharma/nanobanana-cli.git"
  },
  "bugs": {
    "url": "https://github.com/dipendra-sharma/nanobanana-cli/issues"
  },
  "homepage": "https://github.com/dipendra-sharma/nanobanana-cli#readme",
  "dependencies": {
    "@google/genai": "^0.3.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "ora": "^8.1.1",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.6.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
