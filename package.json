{
  "name": "nanobanana-cli",
  "version": "0.2.0",
  "description": "Free AI-powered image generation CLI using Google's Gemini API. Generate, edit, restore images, create icons, patterns, diagrams and more with natural language commands.",
  "type": "module",
  "bin": {
    "nb": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun --format esm",
    "start": "bun run dist/index.js",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run typecheck && bun run build"
  },
  "keywords": [
    "ai",
    "ai-image",
    "ai-art",
    "ai-tools",
    "image-generation",
    "text-to-image",
    "text2image",
    "image-editing",
    "image-generator",
    "cli",
    "command-line",
    "terminal",
    "gemini",
    "gemini-api",
    "google-gemini",
    "generative-ai",
    "artificial-intelligence",
    "nanobanana",
    "icon-generator",
    "pattern-generator",
    "image-editor",
    "bun"
  ],
  "author": "Dipendra Sharma",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dipendra-sharma/nanobanana-cli.git"
  },
  "bugs": {
    "url": "https://github.com/dipendra-sharma/nanobanana-cli/issues"
  },
  "homepage": "https://www.npmjs.com/package/nanobanana-cli",
  "dependencies": {
    "@google/genai": "^1.22.0",
    "chalk": "^5.3.0",
    "commander": "14.0.1",
    "ora": "9.0.0",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.6.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
